{
  "name": "lizalex-site",
  "version": "1.0.0",
  "description": "lizalex-site",
  "homepage": "https://niqdev.github.io/lizalex-site",
  "license": "CC-BY-SA-4.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:niqdev/lizalex-site.git"
  },
  "engines": {
    "node": "6.2.x",
    "npm": "3.9.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles -f favicon.ico ./dist",
    "lint": "eslint src",
    "build": "npm run clean && npm run copy && webpack --config",
    "dist": "cross-env NODE_ENV=production npm run build -- webpack.config.prod.js",
    "start": "npm run build -- webpack.config.dev.js && node devServer.js",
    "publish": "git subtree push --prefix dist origin gh-pages"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "copyfiles": "^0.2.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.21.0",
    "node-sass": "^3.8.0",
    "open": "0.0.5",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "history": "^3.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-intl": "^2.1.3",
    "react-intl-redux": "0.0.10",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux-thunk": "^2.1.0"
  }
